prompt
select vp.data_nachisl
from   vypl_pen_imp_v vp
where  vp.ssylka_fl = &ssylka
and    vp.data_nachisl between to_date(&start_date, 'yyyymmdd') and to_date(&end_date, 'yyyymmdd')
group by vp.data_nachisl
order by vp.data_nachisl
/
