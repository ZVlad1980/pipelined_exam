select pd.source_table, pd.ssylka, pd.status_pen, pd.data_arh
from   sp_pen_dog_v pd
where  pd.ssylka = 7290
/
select op.source_table, op.ssylka_fl, op.nach_deistv, op.okon_deistv, op.primech
from   sp_ogr_pv_v op
where  op.ssylka_fl = 7290
/
select vp.data_nachisl
from   vypl_pen_imp_v vp
where  vp.ssylka_fl = 7290
and    vp.data_nachisl between to_date(19990101, 'yyyymmdd') and to_date(20020630, 'yyyymmdd')
group by vp.data_nachisl
order by vp.data_nachisl
/
